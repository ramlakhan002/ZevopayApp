@model UPIPayoutModel

    <h4 class="card-title">UPI Money Transfer</h4>
   
    <div class="forms-sample">
        <div class="form-group">
        <label asp-for="UPIId">UPI</label>
        <input type="text" asp-for="UPIId" class="form-control" placeholder="ex. xxxxxxxxxx@ybl">
        </div>
        <div class="form-group">
        <label asp-for="FullName">Full Name</label>
        <input type="text" asp-for="FullName" class="form-control" placeholder="Full Name">
        </div>
        <div class="form-group">
        <label asp-for="Amount">Amount</label>
        <input type="number" asp-for="Amount" class="form-control" step="0.01" min="0" placeholder="Amount">
        </div>
        <button type="button" class="btn btn-primary mr-2" id="UPIPayouts_Submit_btn">Submit</button>
    </div>
<script src="~/js/viewsscripts/upipayouts.js"></script>