@model IEnumerable<MemberWalletTransactions>
<div class="table-responsive">

    <table class="table">
        <thead>
            <tr>
                <th>MemebrId</th>
                <th>Date</th>
                <th>Narration</th>
                <th>Credit</th>
                <th>Debit</th>
                <th>Balance</th>
            </tr>
        </thead>
        <tbody>
            @if (Model != null && Model?.Count() > 0)
            {
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item?.MemberId</td>
                        <td>@item?.CreateDate</td>
                        <td>@item?.Narration </td>
                        <td class="text-success">@item?.CrAmount</td>
                        <td class="text-danger">@item?.DrAmount</td>
                        <td>@item?.Balance</td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>